@model AD_Team10.Models.Department

@{
    ViewBag.Title = "ChangeCollectionPoint";
}

<div class="container">
    @using (Html.BeginForm())
    {
        @Html.HiddenFor(x=>x.DepartmentID)
        <div class="form-horizontal">
            <div class="form-group">
                @Html.LabelFor(m => m.CollectionPointID, "CollectionPointID", htmlAttributes: new { @class = "control-label col-md-2" })
                <div class="col-md-10">
                    @Html.DropDownList("CollectionPointID", ViewBag.CollectionPointID as SelectList, "", htmlAttributes: new { @class = "form-control" })
                </div>
            </div>
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <input type="submit" value="Save" class="btn btn-default" />
                </div>
            </div>
        </div>
    }
</div>


